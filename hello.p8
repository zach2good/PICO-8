pico-8 cartridge // http://www.pico-8.com
version 5
__lua__
-- hello world
-- zach toogood

--------------------
-- members
--------------------
screen = {x=0, y=0, w=127, h=127}
colors_val = {0, 3, 11, 7}
colors = {black = 0, dgreen = 3, lgreen = 11, white = 7}

balls = {}

sprite_1 = {frame = 0, x_size = 3, y_size = 3}
sprite_2 = {frame = 3, x_size = 3, y_size = 3}

actor = {sprites = {sprite_1, sprite_2}}

sound = {bounce=0}

start_time = time()

t = 0

--------------------
-- main funcs
--------------------
function _init()
	
	addball()
	addball()
	addball()
 
end

function _update()

 t += 1

	if btn(4) or btn(5) then
		addball()
 end
  
  for i = 1, count(balls) do 
    move_ball(balls[i])
    collide(balls[i])
  end

end

function _draw()
	-- clear
	camera(0,0)
	rectfill(screen.x, screen.y, screen.x+screen.w, screen.y+screen.h, colors.black)

	-- balls
	for i = 1, count(balls) do 
    draw_ball(balls[i])
 end
	
	-- text
	print("hello world", 2, 2, rnd(16))
	print("hello world", 1, 1, colors.white)

	print("press x or z", 1, 2 * 8, colors.white)

	print("balls:", 1, 3 * 8, colors.white)
	print(count(balls), 1, 4 * 8, colors.white)

	print("cpu usage:", 1, 5 * 8, colors.white)
	print(stat(1) * 100, 1, 6 * 8, colors.white)
	
	print("mem usage:", 1, 7 * 8, colors.white)
	print((stat(0)/256) * 100, 1, 8 * 8, colors.white)

  print("", 1, 9 * 8, colors.white) --hacky to get info to print nicely
  info()
	
	-- mr gimmick blinking
	if t < 40 then
	spr(0, 40 + 40, screen.h - 24, 3, 3)
	else
	spr(3, 40 + 40, screen.h - 24, 3, 3)
	end
	
	-- reset ticks
	if t > 60 then
	t = 0
	end
	
end

--------------------
-- game funcs
--------------------
function addball()
add(
    balls, 
    {
      x=rnd(127), 
      y=rnd(127), 
      size=2, 
      x_dir=(rnd(8)-4), 
      y_dir=(rnd(8)-4), 
      color=rnd(15)+1
    })
end


function draw_ball(_ball)
	circfill(_ball.x, _ball.y, _ball.size, _ball.color)
end

function move_ball(_ball)
	x = _ball.x_dir
	y = _ball.y_dir

	_ball.x += x
	_ball.y += y
end

function collide(_ball)
  --top
  if (_ball.y - _ball.size) < (screen.y) then
    _ball.y_dir = -_ball.y_dir
  end
 
  --bottom
  if (_ball.y + _ball.size) > (screen.y + screen.h) then
    _ball.y_dir = -_ball.y_dir
  end

  --left
  if (_ball.x - _ball.size) < (screen.x) then
    _ball.x_dir = -_ball.x_dir
  end

  --right
  if (_ball.x + _ball.size) > (screen.x + screen.w) then
    _ball.x_dir = -_ball.x_dir
  end
end

--------------------
-- utils
--------------------
function randfromtable( t )
    local n = 0
    for i, o in pairs(t) do
        n = n + 1
        if rnd(count(t)) then
            choice = o      
        end
    end
    return choice 
end

function clamp(val,minv,maxv)
  return max(minv,min(val,maxv))
end



